{
  "name": "js-code-creator",
  "version": "1.0.0",
  "description": "Library for code generator",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c -m -w",
    "postinstall": "cross-env NODE_ENV=production rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BysahaOleh/js-code-generator.git"
  },
  "author": "BysahaOleh<olehbusaha@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BysahaOleh/js-code-generator/issues"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coverageDirectory": "coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "mapCoverage": true
  },
  "types": "src/types/index.d.ts",
  "homepage": "https://github.com/BysahaOleh/js-code-generator#readme",
  "dependencies": {
    "@types/jest": "^22.0.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.4",
    "rollup": "^0.53.0",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0"
  }
}
